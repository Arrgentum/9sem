[ ca ]
default_ca = CA_default

[ CA_default ]
dir = ./data
database = $dir/index.txt
certificate = $dir/votintsevak-521-intr.crt
private_key = $dir/votintsevak-521-intr.key
crl_extensions = crl_ext

default_days = 365
default_crl_days = 30
default_md = sha256

[ crl_ext ]
authorityKeyIdentifier=keyid:always,issuer:always

